<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">

    <servlet>
        <servlet-name>SalarieServlet</servlet-name>
        <servlet-class>fr.gestconge.SalarieServlet</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>SalarieServlet</servlet-name>
        <url-pattern>/SalarieServlet</url-pattern>
    </servlet-mapping>

    <servlet>
    <servlet-name>InscriptionServlet</servlet-name>
    <servlet-class>fr.gestconge.InscriptionServlet</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>InscriptionServlet</servlet-name>
        <url-pattern>/InscriptionServlet</url-pattern>
    </servlet-mapping>


</web-app>